{
    "$schema": "https://github.com/hyperledger-labs/fablo/releases/download/2.2.0/schema.json",
    "global": {
      "fabricVersion": "2.5.9",
      "tls": true,
      "engine": "docker",
      "peerDevMode": false,
      "monitoring": {
        "loglevel": "debug"
      },
      "tools": {
        "explorer":true
      }
    },
    "orgs": [
      {
        "organization": {
          "name": "Orderer",
          "domain": "orderer.example.com"
        },
        "orderers": [
          {
            "groupName": "group1",
            "type": "solo",
            "instances": 1
          }
        ],
        "tools": {}
      },
      {
        "organization": {
          "name": "Org1",
          "domain": "org1.example.com"
        },
        "peer": {
          "instances": 2,
          "db": "LevelDb"
        },
        "tools": {
          "fabloRest":true
        }
      }
    ],
    "channels": [
      {
        "name": "channel-mini",
        "orgs": [
          {
            "name": "Org1",
            "peers": [
              "peer0",
              "peer1"
            ]
          }
        ]
      }
    ],
    "chaincodes": [
      {
        "name": "channel_mini_cpir",
        "version": "0.0.1",
        "lang": "golang",
        "channel": "channel-mini",
        "directory": "./cpir_cc/channel_mini_cpir"
      }
  
    ]
  }
  