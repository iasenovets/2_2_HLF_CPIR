	month = may,
	year = {2007},
	note = {ISSN: 2375-1207},
	keywords = {Computer science, Costs, Data privacy, Databases, Information retrieval, Information security, Polynomials, Power system protection, Protocols, Robustness},
	pages = {131--148},
