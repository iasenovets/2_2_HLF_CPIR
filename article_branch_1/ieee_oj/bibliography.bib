	pages = {1107--1114},
	file = {Submitted Version:C\:\\Users\\Thinkpad\\Zotero\\storage\\JB3N38T5\\Dunnett et al. - 2022 - A Trusted, Verifiable and Differential Cyber Threat Intelligence Sharing Framework using Blockchain.pdf:application/pdf},
